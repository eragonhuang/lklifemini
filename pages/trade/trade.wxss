page {
  height: 100%;
}

.container {
  background: #f9f9f9;
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
}

.search {
  height: 88rpx;
  width: 100%;
  padding: 0 30rpx;
  background: #fff;
  display: flex;
  align-items: center;
}

.search .input {
  width: 690rpx;
  height: 56rpx;
  background: #ededed;
  border-radius: 8rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.search .icon {
  background: url('data:image/png;base64,') center no-repeat;
  background-size: 100%;
  width: 28rpx;
  height: 28rpx;
}

.search .txt {
  height: 42rpx;
  line-height: 42rpx;
  color: #666;
  padding-left: 10rpx;
  font-size: 30rpx;
}

.m-menu {
  display: flex;
  height: 180rpx;
  width: 750rpx;
  flex-flow: row nowrap;
  align-items: center;
  justify-content: space-between;
  background-color: #fff;
  padding: 21rpx 0 20rpx 0;
}

.m-menu image {
  display: block;
  width: 88rpx;
  height: 88rpx;
  margin: 0 auto;
  margin-bottom: 12rpx;
}

.m-menu text {
  display: block;
  font-size: 26rpx;
  text-align: center;
  margin: 0 auto;
  line-height: 1;
  color: #333;
  width: fit-content;
}

.m-menu .scroll-view {
  width: 90%;
  white-space: nowrap;
}
.m-menu .foldbtn{
  width: 8%;
  display: flex;
  align-items: center;
  margin-top: 14rpx;
}
.m-menu .foldbtn text{
  display: table-caption;
  letter-spacing: 2px;
}
.m-menu .foldbtn .icon {
  background: url('data:image/svg+xml;base64,');
  width: 34rpx;
  height: 30rpx;
}

.m-menu .scroll-view .scroll-item {
  display: inline-block;
  width: 145rpx;
}

.m-menu .active text {
  background: none repeat scroll 0 0 #2a6f8d;
  border: 2rpx solid #ffffff;
  color: #ffffff;
  padding: 4.68rpx 9.36rpx;
  border-radius:7rpx;
}

.catalog {
  flex: 1;
  width: 100%;
  display: flex;
  border-top: 2rpx solid #fafafa;
}

.catalog .nav {
  width: 164rpx;
  height: 100%;
  background: #fafafa;
}

.catalog .nav .item {
  text-align: center;
  line-height: 90rpx;
  width: 162rpx;
  height: 90rpx;
  color: #333;
  font-size: 28rpx;
  border-left: 6rpx solid #fff;
}

.catalog .nav .item.active {
  color: #ab2b2b;
  font-size: 28rpx;
  border-left: 6rpx solid #ab2b2b;
  border-right: 6rpx solid #fff;
  background: #fff;
}

.catalog .cate {
  border-left: 1px solid #fafafa;
  flex: 1;
  height: 100%;
  padding: 0 30rpx 0 30rpx;
}

.cate-item{
    margin-top: 9rpx;
    height: auto;
    overflow: hidden;
}

.cate-item .h{
    height: 145rpx;
    width: 750rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.cate-item .h .name{
    display: block;
    height: 35rpx;
    margin-bottom: 18rpx;
    font-size: 30rpx;
    color: #333;
}

.cate-item .h .desc{
    display: block;
    height: 24rpx;
    font-size: 24rpx;
    color: #999;
}

.cate-item .b{
  width:590rpx; 
  height: auto;
  overflow: hidden;
}

.cate-item .b .item{
  float: left;
  background: #fff;
  width: 290rpx;
  margin-bottom: 6.25rpx;
  padding-bottom: 22.3rpx;
  height: auto;
  overflow: hidden;
  text-align: center;
}

.cate-item .b .item-b{
  margin-left: 6.25rpx;
}

.cate-item .item .img{
  width: 290rpx;
  height: 242rpx;
}

.cate-item .item .name{
  display: block;
  width: 285rpx;
  height: 35rpx;
  margin: 11.5rpx 0 22rpx 0;
  text-align: center;
  overflow: hidden;
  padding: 0 20rpx;
  font-size: 30rpx;
  color: #333;
}

.cate-item .item .price{
  display: block;
  width: 285rpx;
  height: 30rpx;
  text-align: center;
  font-size: 30rpx;
  color: #b4282d;
}


.goodsitem{
  height: 212rpx;
  width: 720rpx;
  background: #fff;
  padding: 30rpx 30rpx 30rpx 0;
  border-bottom: 1rpx solid #e1e1e17a;
}

.goodsitem:last-child{
  border-bottom: 1px solid #fff;
}

.goodsitem .img{
  float: left;
  width: 150rpx;
  height: 150rpx;
}

.goodsitem .info{
  float: right;
  width: 540rpx;
  height: 150rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  padding-left: 20rpx;
}

.goodsitem .info .name{
  font-size: 28rpx;
  color: #333;
  line-height: 40rpx;
}

.goodsitem .info .subtitle{
  margin-top: 8rpx;
  font-size: 24rpx;
  color: #888;
  line-height: 40rpx;
}

.goodsitem .info .price{
  margin-top: 8rpx;
  margin-right: 140rpx;
  font-size: 28rpx;
  color: #333;
  line-height: 40rpx;
  width: 200rpx;
}

