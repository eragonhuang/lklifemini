@import "/utils/common.wxss";
/*展开浮层样式*/
.attr-pop-box {
  width: 100%;
  height: 95%;
  position: fixed;
  background: rgba(0, 0, 0, .5);
  z-index: 8;
  bottom: 0;
}

.attr-pop {
  width: 100%;
  height: auto;
  max-height: 780rpx;
  padding: 20rpx 0 0 0;
  background: #fff;
  position: fixed;
  z-index: 9;
}

.attr-pop .scroll-item {
  display: inline-block;
  width: 145rpx;
  height: 150rpx;
}
.attr-pop .collapse{
  border-top: 1px solid #f4f4f4;
  margin-top: 15rpx;
  margin-bottom: 18rpx;
}
.attr-pop .collapse button{
  font-weight: bold;
  margin-top: 25rpx;
  padding-bottom: 5rpx;
}
.attr-pop image {
  display: block;
  width: 88rpx;
  height: 88rpx;
  margin: 0 auto;
  margin-bottom: 12rpx;
}
.attr-pop text {
  display: block;
  font-size: 26rpx;
  text-align: center;
  margin: 0 auto;
  line-height: 1;
  color: #333;
  width: fit-content;
}
 .attr-pop button{
  display:flex;
  justify-content: center;
} 
.attr-pop button .textbtn{
  display:flex;
  justify-content: center;
  align-items: center;
  width: 94rpx;
}
.attr-pop .icon {
  background: url('data:image/svg+xml;base64,');
  width: 34rpx;
  height: 30rpx;
  margin-top: 12rpx;
}
.attr-pop .active text {
  background: none repeat scroll 0 0 #2a6f8d;
  border: 1px solid #ffffff;
  color: #ffffff;
  padding: 4.68rpx 9.36rpx;
  border-radius:7rpx;
}


.keywords{
  width: 750rpx;
  height: 60rpx;
  background: #fff;
  padding: 10rpx 10rpx;
  display: inline-block;
  align-items: center;
  justify-content: space-between;
}

.keywords text{
  padding: 0 15rpx;
  align-items: center;
  font-size: 25rpx;
  color: #4F5355;
  margin: 0 10rpx;
  background: none repeat scroll 0 0 #EFF1F3;
  border: 2rpx solid #EFF1F3;
  border-radius:7rpx;
}

.keywords .active{
  color: #2A63B2;
  background: none repeat scroll 0 0 #DFEEFD;
  border: 2rpx solid #DFEEFD;
}

.columns{
  width: 750rpx;
  height: 60rpx;
  background: #fff;
  padding: 10rpx 10rpx;
  padding-left: 120rpx;
  /* display: inline-block;
  align-items: center;
  justify-content: space-between; */
}

.columns .item{
  padding: 0 15rpx;
  align-items: center;
  font-size: 25rpx;
  color: #4F5355;
  margin: 0 30rpx;
  float: left;
}

.columns .arrow{
  display: flex;
  flex-direction: column;
  width: 16rpx;
  height: 32rpx;
  margin-top: 3rpx;
  margin-left: 5rpx;
  padding-left:4rpx;
}
